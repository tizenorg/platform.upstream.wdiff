#: src/mdiff.c:3737
msgid ""
"mdiff - Studies multiple files and searches for similar sequences, it then\n"
"produces possibly detailed lists of differences and similarities.\n"
msgstr ""
"mdiff - Læser flere filer og leder efter lignende sekvenser, dærefter\n"
"vises en muligvis detaljeret liste med forskelle og ligheder.\n"

#: src/mdiff.c:3742
#, c-format
msgid "Usage: %s [OPTION]... [FILE]...\n"
msgstr "Brug: %s [FLAG]... [FIL]...\n"

#: src/mdiff.c:3746
msgid ""
"\n"
"Operation modes:\n"
msgstr ""
"\n"
"Arbejdstilstande:\n"

#: src/mdiff.c:3747
msgid "  -h                     (ignored)\n"
msgstr "  -h                     (ignoreret)\n"

#: src/mdiff.c:3748
msgid "  -v, --verbose          report a few statistics on stderr\n"
msgstr "  -v, --verbose          rapportér en del statistik på standard fejl\n"

#: src/mdiff.c:3749 src/unify.c:295
msgid "      --help             display this help then exit\n"
msgstr "      --help             vis denne hjælpetekst og afslut\n"

#: src/mdiff.c:3750 src/unify.c:296
msgid "      --version          display program version then exit\n"
msgstr "      --version          vis versionsinformation og afslut\n"

#: src/mdiff.c:3752
msgid ""
"\n"
"Formatting output:\n"
msgstr ""
"\n"
"Formatering af resultat:\n"

#: src/mdiff.c:3753
msgid "  -T, --initial-tab       produce TAB instead of initial space\n"
msgstr ""
"  -T, --initial-tab       skriv TAB i stedet for indledende mellemrum\n"

#: src/mdiff.c:3754
msgid "  -l, --paginate          paginate output through `pr'\n"
msgstr "  -l, --paginate          send resultatet gennem 'pr'\n"

#: src/mdiff.c:3755
msgid "  -S, --string[=STRING]   take note of another user STRING\n"
msgstr "  -S, --string[=STRENG]   tag hænsyn til endnu en STRENG\n"

#: src/mdiff.c:3756
msgid ""
"  -V, --show-links        give file and line references in annotations\n"
msgstr ""
"  -V, --show-links        angiv fil- og linjereferencer i annoteringer\n"

#: src/mdiff.c:3757
msgid "  -t, --expand-tabs       expand tabs to spaces in the output\n"
msgstr ""
"  -t, --expand-tabs       erstat tabulatortegn med mellemrum i reultatet\n"

#: src/mdiff.c:3760
msgid ""
"\n"
"Debugging:\n"
msgstr ""
"\n"
"Fejlsøgning:\n"

#: src/mdiff.c:3761
msgid "  -0, --debugging   output many details about what is going on\n"
msgstr "  -0, --debugging   udskriv mange detaljer om hvad som sker\n"

#: src/mdiff.c:3764
msgid ""
"\n"
"Word mode options:\n"
msgstr ""
"\n"
"Flag for ordtilstand:\n"

#: src/mdiff.c:3765 src/wdiff.c:1251 src/wdiff2.c:190
msgid "  -1, --no-deleted           inhibit output of deleted words\n"
msgstr "  -1, --no-deleted           vis ikke slettete ord\n"

#: src/mdiff.c:3766 src/wdiff.c:1252 src/wdiff2.c:191
msgid "  -2, --no-inserted          inhibit output of inserted words\n"
msgstr "  -2, --no-inserted          vis ikke indsatte ord\n"

#: src/mdiff.c:3767 src/wdiff.c:1253 src/wdiff2.c:192
msgid "  -3, --no-common            inhibit output of common words\n"
msgstr "  -3, --no-common            vis ikke fælles ord\n"

#: src/mdiff.c:3768
msgid "  -A, --auto-pager           automatically calls a pager\n"
msgstr "  -A, --auto-pager           videregiv resultatet til en sideviser\n"

#: src/mdiff.c:3769
msgid "  -k, --less-mode            variation of printer mode for \"less\"\n"
msgstr "  -k, --less-mode            speciel variant af -o for 'less'\n"

#: src/mdiff.c:3770
msgid "  -m, --avoid-wraps          do not extend fields through newlines\n"
msgstr "  -m, --avoid-wraps          lad ikke felter løbe over flere linjer\n"

#: src/mdiff.c:3771
msgid "  -o, --printer              overstrike as for printers\n"
msgstr "  -o, --printer              dobbeltskrift som for printere\n"

#: src/mdiff.c:3772
msgid "  -z, --terminal             use termcap as for terminal displays\n"
msgstr "  -z, --terminal             brug termcap til at markere ord\n"

#: src/mdiff.c:3773
msgid "  -O, --item-regexp=REGEXP   compare items as defined by REGEXP\n"
msgstr ""
"  -O, --item-regexp=REGEXP   sammenlign poster som defineres af REGEXP\n"

#: src/mdiff.c:3774
msgid "  -W, --word-mode            compare words instead of lines\n"
msgstr "  -W, --word-mode            sammenlign ord i stedet for linjer\n"

#: src/mdiff.c:3959
msgid "With no FILE, or when FILE is -, read standard input.\n"
msgstr "Uden FIL eller hvis FIL er -, så læs standard ind.\n"

#: src/mdiff.c:3961 src/unify.c:300 src/wdiff.c:1269 src/wdiff2.c:210
msgid "Report bugs to <wdiff-bugs@gnu.org>.\n"
msgstr ""
"Rapportere fejl til <wdiff-bugs@gnu.org>\n"
"Rapportér fejl eller synspunkter på oversættelsen til <dansk@dansk-gruppen."
"dk>.\n"

